/* Rofi theme colors - generated by theme-manager */
/* Import this in your theme: @import "~/.config/themes/generated/rofi/dark.theme" */

* {
    bg0:    {{background.primary}}F2;
    bg1:    {{background.secondary}};
    bg2:    {{background.overlay}}80;
    bg3:    {{ui.focus_active}}F2;
    fg0:    {{foreground.secondary}};
    fg1:    {{foreground.primary}};
    fg2:    {{foreground.secondary}};
    fg3:    {{foreground.muted}};
}
